@model IEnumerable<Manager.Models.Building>

<div class="form-floating mb-3">
    <select class="form-select" id="floatingSelect" aria-label="">
        <option disabled selected>--Chọn toà nhà--</option>
        @foreach (var b in Model)
        {
            <option value="@b.BuildingID">@b.BuildingName</option>
        }
    </select>
    <label for="floatingSelect">Tòa nhà</label>
</div>

