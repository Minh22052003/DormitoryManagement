                    <div class="sidebar-brand">
                        <a asp-controller="Home" asp-action="Index">Ký túc xá</a>
                    </div>
                    <div class="sidebar-brand sidebar-brand-sm">
                        <a asp-controller="Home" asp-action="Index">KTX</a>
                    </div>
                    <ul class="sidebar-menu">
                        <li class="menu-header">Tổng quan</li>
                        <li class="nav-item dropdown active">
                            <a asp-controller="Home" asp-action="Index" class="nav-link"><i class="fas fa-info"></i><span>Thông tin</span></a>
                        </li>
                        <li class="nav-item dropdown">
                            <a asp-controller="Home" asp-action="Notification" class="nav-link"><i class="fas fa-file-alt"></i><span>Thông báo</span></a>
                        </li>
                        <li class="menu-header">Thông tin phòng trọ</li>
                        <li class="nav-item dropdown">
                            <a asp-controller="Home" asp-action="Room" class="nav-link"><i class="fas fa-user-friends"></i> <span>Phòng trọ</span></a>
                        </li>
                        <li class="nav-item dropdown">
                            <a asp-controller="Home" asp-action="Facilities" class="nav-link"><i class="fas fa-couch"></i> <span>Cơ sở vật chất</span></a>
                        </li>
                        <li class="nav-item dropdown">
                            <a asp-controller="Home" asp-action="Track_Rent" class="nav-link"><i class="fas fa-dollar-sign"></i> <span>Theo dõi tiền trọ</span></a>
                        </li>
                        <li class="menu-header">Hỗ trợ</li>
                        <li class="nav-item dropdown">
                            <a asp-controller="Home" asp-action="Sent_Request" class="nav-link"><i class="fas fa-paper-plane"></i> <span>Gửi yêu cầu</span></a>
                        </li>
                        <li class="nav-item dropdown">
                            <a asp-controller="Home" asp-action="Request_Sent" class="nav-link"><i class="far fa-file-alt"></i> <span>Yêu cầu đã gửi</span></a>
                        </li>
                        <li class="menu-header">Khác</li>
                        <li class="nav-item dropdown">
                            <a asp-controller="Home" asp-action="Officer_Information" class="nav-link"><i class="far fa-user"></i> <span>Thông tin cán vụ</span></a>
                        </li>
                </aside>
            </div>

            <!-- Main Content -->
            @RenderBody()
        
    </div>
    </div>

    <!-- General JS Scripts -->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"
